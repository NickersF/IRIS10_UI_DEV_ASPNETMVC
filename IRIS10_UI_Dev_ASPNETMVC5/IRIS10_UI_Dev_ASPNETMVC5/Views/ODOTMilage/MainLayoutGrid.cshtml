
@{
    Layout = null;
}

<!DOCTYPE html>

<html class="h-100">
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>ODOT Mileage Report UI</title>

    @Styles.Render("~/Content/css")

    <script src="https://kendo.cdn.telerik.com/2020.3.1118/js/jquery.min.js"></script>
    <script src="https://kendo.cdn.telerik.com/2020.3.1118/js/jszip.min.js"></script>
    <script src="https://kendo.cdn.telerik.com/2020.3.1118/js/kendo.all.min.js"></script>
    <script src="https://kendo.cdn.telerik.com/2020.3.1118/js/kendo.aspnetmvc.min.js"></script>
    <script src="@Url.Content("~/Scripts/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/Scripts/kendo.modernizr.custom.js")"></script>
</head>
<body class="h-100">

    <!-- #region Parent Content Wrapper -->
    <main id="mainLayoutWrapper" class="d-flex flex-column h-100">

        <section id="mainHeader" class="flextest-light-blue">mainHeader</section>
        <section id="mainContentWrapper" class="row no-gutters flex-grow-1">
            <section id="mainSideBar" class="col-md-auto flextest-medium-blue">
                <p>mainSideBar</p>
                <button id="closeSideBar_btn" class="k-button">Close Side Bar</button>
            </section>
            <section id="mainContentSection" class="col-md flex-grow-1 flextest-dark-blue">
                <p>mainContentSecttion</p>
                <button id="openSideBar_btn" class="k-button">Show Side Bar</button>
            </section>
        </section>

    </main>
    <!-- #endregion -->

    <script src="~/Scripts/custom/ODOT_UI_MainEvents.js"></script>
</body>
</html>