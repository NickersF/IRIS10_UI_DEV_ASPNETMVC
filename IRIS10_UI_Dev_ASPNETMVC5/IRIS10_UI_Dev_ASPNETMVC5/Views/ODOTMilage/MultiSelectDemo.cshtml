
@{
    Layout = null;
}

<!DOCTYPE html>

<html class="h-100">
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>ODOT Mileage Report UI</title>

    @Styles.Render("~/Content/css")

    @*<link href="https://kendo.cdn.telerik.com/2021.2.511/styles/kendo.default.min.css" rel="stylesheet" type="text/css" />*@
    @*<link href="@Url.Content("~/Content/bootstrap.min.css")" rel="stylesheet" type="text/css" />*@
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="https://kendo.cdn.telerik.com/2021.2.511/js/jquery.min.js"></script>
    <script src="https://kendo.cdn.telerik.com/2021.2.511/js/jszip.min.js"></script>
    <script src="https://kendo.cdn.telerik.com/2021.2.511/js/kendo.all.min.js"></script>
    <script src="https://kendo.cdn.telerik.com/2021.2.511/js/kendo.aspnetmvc.min.js"></script>
    <script src="@Url.Content("~/Scripts/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/Scripts/kendo.modernizr.custom.js")"></script>
</head>
<body class="h-100">

    <div class="container-fluid">
        <div class="row">
            <div class="col-6">
                <button id="clearAll_btn" class="btn btn-primary">Remove All</button>
                @*<button id="clearSelection_btn" class="btn btn-primary">Remove Selected Items</button>*@
                <ul id="UserselectedCountiesList" class="list-group">
                </ul>
                <button id="queryData_btn" class="btn btn-primary">Run Query Against Selection</button>

                <div class="form-check">
                    <input class="form-check-input" type="checkbox" value="" id="defaultCheck1">
                    <label class="form-check-label" for="defaultCheck1">
                        Default checkbox
                    </label>
                </div>

            </div>
            <div class="col-6">
                <object id="svgMapObject_Demo" type="image/svg+xml" class="w-50" data="~/Images/Sample_Counties.svg"></object>

            </div>
        </div>
    </div>

    <script src="~/Scripts/custom/ODOT_UI_MultiSelect.js"></script>
    <script src="~/Scripts/custom/ODOT_UI_MainEvents.js"></script>

</body>
</html>
