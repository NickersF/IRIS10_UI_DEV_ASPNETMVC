        <div class="col-3 border-left">
            <div class="form-group">
                <label for="EditTypeOfAccess">Type of Access:</label>
                <div>
                    @(Html.Kendo().DropDownListFor(m => m.PermitTypeOfAccess_Key)
                            .Name("EditTypeOfAccess")
                            .DataTextField("Text")
                            .DataValueField("Value")
                            .AutoWidth(true)
                            .DataSource(d =>
                            {
                                d.Read(r => r.Action("GetAccessTypeList", "PermitMain").Type(HttpVerbs.Get));
                            }))
                </div>
            </div>
        </div>
    </div>

