    <div class="row">
        <div class="col">
            <h5 class="border-bottom">Identification Data</h5>
        </div>
    </div>
    <div class="row">
        <div class="col-3 border-left">
            <div class="form-group">
                <label for="EditPermitNumber">Permit Number:</label>
                <div>
                    @(Html.Kendo().TextBox()
                            .Name("EditPermitNumber")
                            )
                </div>
            </div>
        </div>
        <div class="col-3 border-left">
            <div class="form-group">
                <label for="EditAuthorizationNumber">Authorization Number:</label>
                <div>
                    @(Html.Kendo().TextBoxFor(m => m.AuthorizationNumber)
                            .Name("EditAuthorizationNumber")
