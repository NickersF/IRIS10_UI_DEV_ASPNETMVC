            </div>
        </div>
        <div class="col-3 border-left">
            <div class="form-group">
                <label for="EditApprovedBy">Approved By:</label>
                <div>
                    @(Html.Kendo().TextBox().Name("EditApprovedBy").Placeholder("Enter approving user"))
                </div>
            </div>
        </div>
        <div class="col-3 border-left">
            <div class="form-group">
                <label for="EditApprovedDate">Approved Date:</label>
                <div>
                    @(Html.Kendo().DatePicker().Name("EditApprovedDate"))
