@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="permitGeneralFields" class="container-fluid mt-3 mb-3">
    @* Identification data start *@
    <div class="row">
        <div class="col">
            <h5 class="border-bottom">Identification Data</h5>
        </div>
    </div>
    <div class="row">
        <div class="col-3 border-left">
            <div class="form-group">
                <label for="EditPermitNumber">Permit Number:</label>
                <div>
                    @(Html.Kendo().TextBox().Name("EditPermitNumber").Placeholder("Enter permit number"))
                </div>
            </div>
        </div>
        <div class="col-3 border-left">
            <div class="form-group">
                <label for="EditAuthorizationNumber">Authorization Number:</label>
                <div>
                    @(Html.Kendo().TextBox().Name("EditAuthorizationNumber").Placeholder("Enter authorization number"))
                </div>
            </div>
        </div>
        <div class="col-3 border-left">
            <div class="form-group">
                <label for="EditApplicationNumber">Application Number:</label>
                <div>
                    @(Html.Kendo().TextBox().Name("EditApplicationNumber").Placeholder("Enter application number"))
                </div>
            </div>
        </div>
        <div class="col-3 border-left">
            <div class="form-group">
                <label for="EditTypeOfAccess">Type of Access:</label>
                <div>
                    @(Html.Kendo().DropDownList()
                            .Name("EditTypeOfAccess")
                            .DataTextField("Text")
                            .DataValueField("Value")
                            .AutoWidth(true)
                            .DataSource(d =>
                            {
                                d.Read(r => r.Action("GetAccessTypeList", "PermitMain").Type(HttpVerbs.Get));
                            }))
                </div>
            </div>
        </div>
    </div>

    <div class="row mt-3">
        <div class="col-3 border-left">
            <div class="form-group">
                <label for="EditIssuedBy">Issued By:</label>
                <div>
                    @(Html.Kendo().TextBox().Name("EditIssuedBy").Placeholder("Enter issuing user"))
                </div>
            </div>
        </div>
        <div class="col-3 border-left">
            <div class="form-group">
                <label for="EditIssueDate">Issue Date:</label>
                <div>
                    @(Html.Kendo().DatePicker().Name("EditIssueDate"))
